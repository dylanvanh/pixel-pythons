# Mempool API for UTXO and transaction data
MEMPOOL_URL=https://mempool.space/api

# Ordiscan API for ordinals data
ORDISCAN_URL=https://api.ordiscan.com
ORDISCAN_API_KEY=your_ordiscan_api_key_here

# Oracle Configuration (Bitcoin wallet for signing transactions)
# Private key in WIF format
ORACLE_PRIVATE_KEY_WIF=your_oracle_private_key_wif_here
# Compressed public key corresponding to the private key above
ORACLE_COMPRESSED_PUBLIC_KEY=your_oracle_compressed_public_key_here
# Taproot address for the oracle wallet
ORACLE_TAPROOT_ADDRESS=your_oracle_taproot_address_here

# Supabase Configuration
# Supabase project URL
SUPABASE_URL=https://your-project-id.supabase.co
# Service role key with full database access
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Client side configuration
# The parent inscription ID for the ordinals collection
NEXT_PUBLIC_PARENT_INSCRIPTION_ID=your_parent_inscription_id_here
# Base URL of your deployed application
NEXT_PUBLIC_BASE_URL=http://localhost:3001
